{
  "compilerOptions": {
    "allowJs": true,
    "checkJs": false,
    "module": "commonjs",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "sourceMap": true,
    "strict": true,
    "baseUrl": ".",
    "outDir": ".webpack/main",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "jsx": "react",
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "paths": {
      "*": [
        "node_modules/*"
      ]
    }
  },
  "exclude": [
    "node_modules",
    ".webpack",
    "out"
  ]
}
